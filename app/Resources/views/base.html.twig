    <script type="text/javascript" src="{{ asset('bundles/acmeblog/socialshareprivacy/socialshareprivacy.js') }}"></script>
    <script type="text/javascript">

  jQuery(document).ready(function($){
  
    if($('#socialshareprivacy').length > 0){
      $('#socialshareprivacy').socialSharePrivacy({
        'services' : {
    'facebook' : {
      'sharer' : {
        'status'    : 'on',
	display_name   : 'My Inline Service',
  
        'dummy_img' : '/bundles/acmeblog/socialshareprivacy/images/dummy_facebook_share_de.png',
        'img'       : '/bundles/acmeblog/socialshareprivacy/images/facebook_share_de.png'
      }
    },
    'twitter' : {
                'dummy_img' : '/bundles/acmeblog/socialshareprivacy/images/dummy_twitter.png',
    },
    'gplus' : { 
                'dummy_img' : '/bundles/acmeblog/socialshareprivacy/images/dummy_gplus.png',
    }
  },
        "css_path"  : "/bundles/acmeblog/socialshareprivacy/socialshareprivacy.css",
        "lang_path" : "/bundles/acmeblog/socialshareprivacy/lang/",
        "language"  : "de",
        'alignment' : 'vertical'
      });
    }
  });
</script>
  </head>
